{
  "name":"NSD simpleClientServer",
  "vendor":"IITH",
  "version":"1.0",
  "vld":[
    {
      "name":"private"
    }
  ],
  "vnfd":[
    {
      "name":"simple-client",
      "vendor":"IITH",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh",
            "configure_host.sh",
            "compileFiles.sh"
          ]
        },
        {
          "event":"CONFIGURE",
          "lifecycle_events":[
            "server_configure.sh"
          ]
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "ubuntu"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "virtual_link_reference":"private",
                  "interfaceId":0
                }
              ]
            }
          ],
          "vimInstanceName":["temp"]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.small"
        }
      ],
      "type":"client",
      "endpoint":"generic",
      "vnfPackageLocation":"https://github.com/tulja/simpleClientServerOpenBaton.git"
    },
    {
      "name":"simple-server",
      "vendor":"IITH",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh",
            "configure_host.sh",
            "compileFiles.sh",
            "startServer.sh"
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "ubuntu"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "virtual_link_reference":"private",
                  "interfaceId":0
                }
              ]
            }
          ],
          "vimInstanceName":["temp"]
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.small"
        }
      ],
      "type":"server",
      "endpoint":"generic",
      "vnfPackageLocation":"https://github.com/tulja/simpleClientServerOpenBaton.git"
    }
  ],
  "vnf_dependency":[
    {
      "source":{
        "name":"simple-server"
      },
      "target":{
        "name":"simple-client"
      },
      "parameters":[
        "private"
      ]
    }
  ]
}
